pipeline {
    agent { label 'ubuntu' }

    stages {
        stage('Doker version') {
            steps {
                sh "echo $USER"
                sh "docker version"
            }
        }
        stage('Checkout') {
            steps {
                git branch: 'main',
                    url: 'https://github.com/oleg-vodyak/jenkins.git'
                    sh 'pwd'
                    sh 'ls -all'
            }
        }
    }
}